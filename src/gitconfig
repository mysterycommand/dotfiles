[user]
  email = matt@mysterycommand.com
  name = Matt Hayes
[push]
  default = simple
[filter "media"]
  clean = git-media-clean %f
  smudge = git-media-smudge %f
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
  required = true
	process = git-lfs filter-process
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[core]
  excludesfile = ~/.gitexcludes
[alias]
  br = branch
  co = checkout
  cm = commit
  rb = rebase
  st = status
  df = diff
  lg = log -p
  fap = fetch --all --prune
  mom = "!git fetch; git merge origin/master"
  mum = "!git fetch; git merge upstream/master"
  dfs = diff --staged
[pager]
  branch = false
  diff = false
[pull]
  rebase = false
